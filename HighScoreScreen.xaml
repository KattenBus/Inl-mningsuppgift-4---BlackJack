<Window x:Class="GruppInlämning_4___BlackJack.HighScoreScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GruppInlämning_4___BlackJack"
        mc:Ignorable="d"
        Title="HighScoreScreen" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Highscore.png"/>
        </Grid.Background>
        <ListBox x:Name="HighScoreListBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="285,156,285,50" Background="Black" Foreground="White"/>
        <Label x:Name ="HighScoreLabel" Content="Label" HorizontalAlignment="Left" Margin="334,109,0,0" VerticalAlignment="Top" Width="114" FontSize="24" Foreground="#FFF7F3F3" Background="#FF1B1A1A"/>

    </Grid>
</Window>
