<Window x:Class="GruppInlämning_4___BlackJack.GameMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GruppInlämning_4___BlackJack"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/GameMenu.png"/>
        </Grid.Background>
        <Button x:Name="GoToHighScoreScreenButton" Content="HighScore" HorizontalAlignment="Left" Margin="381,320,0,0" VerticalAlignment="Top" Click="GoToHighScoreScreenButton_Click" FontFamily="Franklin Gothic Heavy" FontSize="14" Height="31" Width="89"/>
        <Button x:Name="GoToBlackJackButton" Content="BlackJack" HorizontalAlignment="Left" Margin="383,265,0,0" VerticalAlignment="Top" Click="GoToBlackJackButton_Click" FontFamily="Franklin Gothic Heavy" Height="35" Width="87" FontSize="14"/>
        <Button x:Name="logOutButton" Content="Log out" HorizontalAlignment="Left" Margin="381,371,0,0" VerticalAlignment="Top" Click="logOutButton_Click" FontFamily="Franklin Gothic Heavy" Height="30" Width="89" FontSize="14"/>
        <Label x:Name="welcomeLabel" Content="" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
